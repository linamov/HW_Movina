<!DOCTYPE html>
<html lang="uk">
<head>
  <meta charset="UTF-8">
  <title>Домашнє завдання</title>
</head>
<body>
  <h1>Типи даних</h1>

  <script>
    let текст = "Привіт";           // текст, тип string
    let число = 123;                // число, тип number
    let правда = true;              // логічне значення, тип boolean
    let нічого = null;              // нічого, тип null (але покаже "object")
    let невідоме;                   // не визначено, тип undefined
    let символ = Symbol("id");      // символ, тип symbol
    let велике = 123456789n;        // велике число, тип bigint
    let обʼєкт = { імʼя: "Оля" };   // обʼєкт, тип object
    let функція = function() {};    // функція, тип function

    console.log(typeof текст);
    console.log(typeof число);
    console.log(typeof правда);
    console.log(typeof нічого);
    console.log(typeof невідоме);
    console.log(typeof символ);
    console.log(typeof велике);
    console.log(typeof обʼєкт);
    console.log(typeof функція);
  </script>
</body>
</html>
